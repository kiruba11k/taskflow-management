{
  "name": "Team",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Team name"
    },
    "description": {
      "type": "string",
      "description": "Team purpose"
    },
    "team_leaders": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "team_members": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "is_active": {
      "type": "boolean",
      "default": true
    }
  },
  "required": [
    "name"
  ]
}